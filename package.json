{
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "commit": "git-cz",
    "precommit": "yarn prettier:write && yarn ts && lint-staged",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "test:log": "jest --no-coverage >& ./logs/jest.log",
    "update:packages": "yarn upgrade-interactive --latest",
    "lint": "eslint --fix --ext=jsx,ts,tsx src",
    "prettier:write": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "prepare": "tsdx build",
    "ts": "npx tsc"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "name": "upstamps-react",
  "author": "Vitor Amaral",
  "module": "dist/upstamps-react-sdk.esm.js",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@types/jest": "^26.0.15",
    "@types/localforage": "^0.0.34",
    "@types/node": "^14.14.0",
    "@types/prettier": "^2.1.5",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.0",
    "jest": "^26.6.0",
    "lint-staged": "^10.4.2",
    "prettier": "^2.1.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "ts-jest": "^26.4.1",
    "tsdx": "^0.14.1",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "Utils": "^1.0.39",
    "localforage": "^1.9.0",
    "lodash.isequal": "^4.5.0",
    "tslib": "^2.0.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && tsc"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,json,css,md}": [
      "eslint --fix --ext=jsx,ts,tsx src",
      "prettier --write",
      "jest --findRelatedTests --coverage=false",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 edge version"
    ]
  }
}
